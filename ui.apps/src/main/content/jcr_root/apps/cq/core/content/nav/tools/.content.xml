<?xml version="1.0" encoding="UTF-8"?>
<jcr:root
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured">
    <etoolbox jcr:primaryType="nt:unstructured"
        jcr:title="EToolbox">
        <lists
            jcr:primaryType="nt:unstructured"
            jcr:title="EToolbox Lists"
            jcr:description="Manage collections of structured data"
            order="2000"
            href="/etoolbox-lists.html"
            icon="etoolbox-lists"
            sling:resourceType="crx/core/components/welcome/tool"/>
        <configurator
            jcr:primaryType="nt:unstructured"
            jcr:title="EToolbox Configurator"
            jcr:description="Edit AEM components configurations"
            order="2001"
            href="/etoolbox/config.html"
            icon="etoolbox-configurator"
            sling:resourceType="etoolbox-authoring-kit/components/shortcut">
            <granite:rendercondition
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etoolbox-authoring-kit/configurator/components/rendercondition"
                feature="eak.configurator.showShortcut"/>
        </configurator>
    </etoolbox>
</jcr:root>
