<div class="features-container container d-flex mb-150">
    <div class="features-header">
        <h3>{{ landing.features.title }}</h3>
    </div>
    <div class="features-content">
        <ul class="features-list">
        {% for feature in landing.features.items %}
            <li class="features-item fw-300">{{ feature }}</li>
        {% endfor %}
        </ul>
    </div>
</div>
