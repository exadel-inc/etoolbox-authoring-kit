<div class="bv-container mx-0 mb-150">
  <ul class="bv-list">
    {% for value in landing.businessValue %}
      <li class="bv-block mx-2">
        <div role="presentation">
          {% include "../../../static/assets/helpers/light-slash.svg" %}
        </div>
        <div class="bv-item mx-auto my-auto">
          <strong class="d-block mb-1">{{ value.emphasized }}</strong>
          <p class="fw-400 vertical-align">{{ value.content }}</p>
        </div>
      </li>
    {% endfor %}
  </ul>
</div>
