  "main": "index.js",
  "scripts": {
    "build": "concurrently \"npm:build-eleventy\" \"webpack\" \"npm:build-lessc\"",
    "start": "npm run prebuild && npm run build-lessc && webpack && npm run watch-eleventy",
    "prebuild": "node directory.reader.js && node sidebar.template.js",
    "watch-webpack": "webpack --watch",
    "watch-eleventy": "npx @11ty/eleventy --serve --port=3005",
    "build-eleventy": "npx @11ty/eleventy",
    "build-lessc": "lessc src/localdev.less dist/bundles/localdev.css",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:css\" \"npm run lint:spaces\" && node build/catlog.js \"Linting passed\"",
    "lint:js": "eslint src/**/*.ts --max-warnings 3",
    "lint:css": "stylelint src/**/*.less",
    "lint:spaces": "lintspaces ./{src,views,controller}/**/*.{html,less,json} -n -t -d spaces"
  },
