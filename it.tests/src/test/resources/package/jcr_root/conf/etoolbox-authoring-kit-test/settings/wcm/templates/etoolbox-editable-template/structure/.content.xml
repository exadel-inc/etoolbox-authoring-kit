          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:lastModified="{Date}2022-09-20T12:00:00.000Z"
        cq:lastModifiedBy="admin"
        cq:template="/conf/etoolbox-authoring-kit-test/settings/wcm/templates/etoolbox-editable-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="etoolbox-authoring-kit-test/components/pages/editable-template-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <grid_0
                editable="{Boolean}true"
                jcr:created="{Date}2022-09-20T12:00:00.000Z"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2022-09-20T12:00:00.000Z"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
            <grid_1
                editable="{Boolean}true"
                jcr:created="{Date}2022-09-20T12:00:00.000Z"
                jcr:createdBy="admin"
                jcr:lastModified="{Date}2022-09-20T12:00:00.000Z"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
