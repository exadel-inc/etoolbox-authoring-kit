<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="Test Component">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <forceIgnoreFreshnessOpen
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etoolbox-authoring-kit/components/authoring/ignoreFreshnessToggler"/>
            <accordion jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <first jcr:primaryType="nt:unstructured"
                           sling:resourceType="granite/ui/components/coral/foundation/container"
                           jcr:title="First">
                        <items jcr:primaryType="nt:unstructured">
                            <mySwitch
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                checked="{Boolean}false"
                                disabled="{Boolean}false"
                                fieldLabel="Switch"
                                ignoreData="{Boolean}false"
                                name="./mySwitch"
                                offText="Off"
                                onText="On"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                        </items>
                    </first>
                    <second jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            jcr:title="Second">
                        <items jcr:primaryType="nt:unstructured">
                            <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}false"
                                fieldLabel="Text"
                                name="./text"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                        </items>
                    </second>
                </items>
            </accordion>
            <forceIgnoreFreshnessClose
                jcr:primaryType="nt:unstructured"
                sling:resourceType="etoolbox-authoring-kit/components/authoring/ignoreFreshnessToggler"/>
        </items>
    </content>
</jcr:root>
