<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="ImageUpload Widget Dialog"
          height="{Double}480.0"
          width="{Double}560.0">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <file1 jcr:primaryType="nt:unstructured"
                           sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                           name="./file1"
                           allowUpload="{Boolean}false"
                           disabled="{Boolean}false"
                           fileNameParameter="./image/fileName"
                           fileReferenceParameter="./image/fileRef"
                           mimeTypes="[.png]"
                           renderHidden="{Boolean}false"
                           required="{Boolean}false"
                           sizeLimit="{Long}100000"/>
                    <file2 jcr:primaryType="nt:unstructured"
                           sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                           name="./file2"
                           allowUpload="{Boolean}true"
                           disabled="{Boolean}false"
                           fileNameParameter="./image/fileName"
                           fileReferenceParameter="./image/fileRef"
                           mimeTypes="[.png]"
                           icon="search"
                           renderHidden="{Boolean}false"
                           required="{Boolean}false"
                           sizeLimit="{Long}100000"
                           viewInAdminURI="admin/uri"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
