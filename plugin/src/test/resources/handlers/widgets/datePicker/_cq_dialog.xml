<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          width="{Double}560.0"
          height="{Double}480.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="DatePicker Widget Dialog"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <currentDate sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                 jcr:primaryType="nt:unstructured"
                                 afterSelector="after"
                                 beforeSelector="before"
                                 disabled="{Boolean}false"
                                 displayTimezoneMessage="{Boolean}true"
                                 displayedFormat="YYYY-MM-DD[T]HH:mm:ss.000"
                                 maxDate="30.04.2020 12:10"
                                 minDate="01.01.2019 00:00"
                                 name="./date"
                                 renderHidden="{Boolean}false"
                                 required="{Boolean}false"
                                 type="datetime"
                                 typeHint="String"
                                 valueFormat="DD.MM.YYYY HH:mm"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
