<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog"
    jcr:title="My AEM Component"
    helpPath="https://www.google.com/search?q=my+aem+component"
    height="{Double}600.0"
    width="{Double}800.0">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured"
                       emptyValueProperty="">
                    <field jcr:primaryType="nt:unstructured"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                           name="./field"
                           disabled="{Boolean}false"
                           renderHidden="{Boolean}false"
                           required="{Boolean}false"
                           simpleProperty="value">
                        <ad jcr:primaryType="nt:unstructured"
                            wrongFieldName="non-latin символы"/>
                    </field>
                    <siblingNode jcr:primaryType="nt:unstructured"
                                 newProperty="value"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
