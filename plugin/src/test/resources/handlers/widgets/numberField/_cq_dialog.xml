<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          width="{Double}560.0"
          height="{Double}480.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="NumberField Widget Dialog"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <number1 jcr:primaryType="nt:unstructured"
                             disabled="{Boolean}false"
                             fieldLabel="Valid number"
                             name="./number1"
                             renderHidden="{Boolean}false"
                             required="{Boolean}false"
                             min="{Double}-10.0"
                             max="{Double}10.0"
                             step="{Double}1.0"
                             value="0"
                             sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                    <number2 jcr:primaryType="nt:unstructured"
                             disabled="{Boolean}false"
                             fieldLabel="No value preset"
                             name="./number2"
                             renderHidden="{Boolean}false"
                             required="{Boolean}false"
                             min="{Double}-10.0"
                             max="{Double}10.0"
                             step="{Double}2.0"
                             sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
