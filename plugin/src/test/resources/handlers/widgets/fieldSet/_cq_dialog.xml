<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="FieldSet Widget Dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <fieldSet1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <textField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./textField21"
                                disabled="{Boolean}false"
                                fieldDescription="Field 1 description"
                                fieldLabel="Field 1 Label"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <checkboxField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./checkboxField21"
                                autosubmit="{Boolean}false"
                                checked="{Boolean}false"
                                disabled="{Boolean}false"
                                fieldDescription="Field 2 description"
                                fieldLabel="Field 2 Label"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                text="Field 2 Label"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <textFieldEraser
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./textField21@Delete"
                                disabled="{Boolean}false"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                        </items>
                    </fieldSet1>
                    <fieldSet2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <textField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./second_textField"
                                disabled="{Boolean}false"
                                fieldDescription="Field 1 description"
                                fieldLabel="Field 1 Label"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <checkboxField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./second_checkboxField"
                                autosubmit="{Boolean}false"
                                checked="{Boolean}false"
                                disabled="{Boolean}false"
                                fieldDescription="Field 2 description"
                                fieldLabel="Field 2 Label"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                text="Field 2 Label"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                            <textFieldEraser
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                name="./second_textField@Delete"
                                disabled="{Boolean}false"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <extraField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./second_extraField"
                                disabled="{Boolean}false"
                                fieldDescription="Field 3 description"
                                fieldLabel="Field 3 Label"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                        </items>
                    </fieldSet2>
                    <fieldSet3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldsetTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./node1_fieldsetTitle"
                                disabled="{Boolean}false"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <multifieldItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                disabled="{Boolean}false"
                                multifieldSpecial="This is added to Multifields"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    name="./node1_multifieldTitle"
                                    disabled="{Boolean}false"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"/>
                            </multifieldItems>
                        </items>
                    </fieldSet3>
                    <fieldSet4
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldsetTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}false"
                                name="./node2_fieldsetTitle"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <multifieldItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                disabled="{Boolean}false"
                                multifieldSpecial="This is added to Multifields"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./node2_multifieldItems">
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            name="title"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false"/>
                                        <description
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            name="description"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false"/>
                                    </items>
                                </field>
                            </multifieldItems>
                        </items>
                    </fieldSet4>
                    <fieldSet5
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        multifieldSpecial="This is added to Multifields">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                            <items jcr:primaryType="nt:unstructured">
                                <fieldsetTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    disabled="{Boolean}false"
                                    name="node3/fieldsetTitle"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"/>
                                <multifieldItems
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    disabled="{Boolean}false"
                                    multifieldSpecial="This is added to Multifields"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false">
                                    <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        disabled="{Boolean}false"
                                        name="./node3/multifieldTitle"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"/>
                                </multifieldItems>
                            </items>
                        </field>
                    </fieldSet5>
                    <fieldSet6
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        disabled="{Boolean}false"
                        multifieldSpecial="This is added to Multifields"
                        renderHidden="{Boolean}false"
                        required="{Boolean}false">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./fieldSet6">
                            <items jcr:primaryType="nt:unstructured">
                                <fieldsetTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    disabled="{Boolean}false"
                                    name="node4/fieldsetTitle"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"/>
                                <multifieldItems
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                    composite="{Boolean}true"
                                    disabled="{Boolean}false"
                                    multifieldSpecial="This is added to Multifields"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false">
                                    <field
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        name="./node4/multifieldItems">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                disabled="{Boolean}false"
                                                name="title"
                                                renderHidden="{Boolean}false"
                                                required="{Boolean}false"/>
                                            <description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                disabled="{Boolean}false"
                                                name="description"
                                                renderHidden="{Boolean}false"
                                                required="{Boolean}false"/>
                                        </items>
                                    </field>
                                </multifieldItems>
                            </items>
                        </field>
                    </fieldSet6>
                    <fieldSet7
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                        <items jcr:primaryType="nt:unstructured">
                            <fieldsetTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./node5/fieldsetTitle"
                                disabled="{Boolean}false"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                            <multifieldItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                disabled="{Boolean}false"
                                multifieldSpecial="This is added to Multifields"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./node5/multifieldItems">
                                    <items jcr:primaryType="nt:unstructured">
                                        <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            disabled="{Boolean}false"
                                            name="title"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false"/>
                                        <nestedFieldSet
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                            disabled="{Boolean}false"
                                            name="nestedFieldSet"
                                            renderHidden="{Boolean}false"
                                            required="{Boolean}false">
                                            <items jcr:primaryType="nt:unstructured">
                                                <multifieldTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    disabled="{Boolean}false"
                                                    name="node6_multifieldTitle"
                                                    renderHidden="{Boolean}false"
                                                    required="{Boolean}false"/>
                                            </items>
                                        </nestedFieldSet>
                                    </items>
                                </field>
                            </multifieldItems>
                        </items>
                    </fieldSet7>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
