<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="Fixed Columns Widget Dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <fixedColumns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <first jcr:primaryType="nt:unstructured"
                                   sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <nestedText
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        name="./nestedText"
                                        disabled="{Boolean}false"
                                        fieldLabel="Nested text"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"/>
                                    <number
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        name="./number"
                                        disabled="{Boolean}false"
                                        fieldLabel="Number"
                                        max="{Double}10.0"
                                        min="{Double}-10.0"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"
                                        step="{Double}1.0"
                                        value="0"/>
                                    <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        name="./text"
                                        disabled="{Boolean}false"
                                        fieldLabel="Text"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"/>
                                </items>
                            </first>
                            <second jcr:primaryType="nt:unstructured"
                                   sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <nestedDescription
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        name="./nestedDescription"
                                        autofocus="{Boolean}false"
                                        disabled="{Boolean}false"
                                        fieldLabel="Nested description"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"
                                        resize="none"
                                        rows="{Long}5"/>
                                    <nestedFixedColumns
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                        <items jcr:primaryType="nt:unstructured">
                                            <third
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <extraNestedText
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        name="./extraNestedText"
                                                        disabled="{Boolean}false"
                                                        fieldLabel="Extra-Nested Text"
                                                        renderHidden="{Boolean}false"
                                                        required="{Boolean}false"/>
                                                </items>
                                            </third>
                                        </items>
                                    </nestedFixedColumns>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        name="./description"
                                        autofocus="{Boolean}false"
                                        disabled="{Boolean}false"
                                        fieldLabel="Description"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"
                                        resize="none"
                                        rows="{Long}5"/>
                                </items>
                            </second>
                        </items>
                    </fixedColumns>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
