<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:title="Test Component"
          height="{Double}480.0"
          width="{Double}560.0">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <childTitle jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./childTitle"
                                fieldLabel="Title Child"
                                disabled="{Boolean}false"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"/>
                    <anotherFieldChild jcr:primaryType="nt:unstructured"
                                       sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                       name="./anotherFieldChild"
                                       fieldLabel="Another Field Child"
                                       disabled="{Boolean}false"
                                       renderHidden="{Boolean}false"
                                       required="{Boolean}false"/>
                    <untouchedField jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    name="./untouchedField"
                                    fieldLabel="Untouched Field"
                                    disabled="{Boolean}false"
                                    renderHidden="{Boolean}false"
                                    required="{Boolean}false"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
