<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="Slider Widget Dialog"
          width="{Double}560.0"
          height="{Double}480.0">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <number jcr:primaryType="nt:unstructured"
                            sling:resourceType="etoolbox-authoring-kit/components/authoring/slider"
                            name="./number"
                            fieldLabel="Number"
                            disabled="{Boolean}false"
                            ranged="{Boolean}true"
                            renderHidden="{Boolean}false"
                            required="{Boolean}false"
                            startValue="{Long}4"
                            endValue="{Long}10"
                            min="{Long}10"
                            max="{Long}200"
                            step="{Long}2"
                            orientation="vertical"
                            filled="{Boolean}true"
                            value="{Long}14">
                        <items jcr:primaryType="nt:unstructured">
                            <option_1 jcr:primaryType="nt:unstructured" text="First" value="{Long}1"/>
                            <option_2 jcr:primaryType="nt:unstructured" text="Second" value="{Long}2"/>
                            <option_3 jcr:primaryType="nt:unstructured" text="Third" value="{Long}3"/>
                            <option_4 jcr:primaryType="nt:unstructured" text="Fourth" value="{Long}4"/>
                            <option_5 jcr:primaryType="nt:unstructured" text="Fifth" value="{Long}5"/>
                        </items>
                    </number>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
